{
  "expo": {
    "name": "push2",
    "slug": "push2",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "3.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0

    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {      
      "package": "com.fazpedidoPush.app", 
      
      "versionCode": 2,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{
            "scheme": "fazpedidoPush",
            "host":"fazpedidoPush"
          }],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },    
    "ios": {
      "supportsTablet": true
    },
    "notification":{
      "icon": "./assets/icon-notif.png",
      "color": "#000000"
    }

  }
}
